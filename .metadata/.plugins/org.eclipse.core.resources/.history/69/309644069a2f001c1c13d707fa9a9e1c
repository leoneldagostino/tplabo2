#ifndef ARRAYEMPLOYEES_H_
#define ARRAYEMPLOYEES_H_

typedef struct
{
 int id;
 char nombre[51];
 char apellido[51];
 float salario;
 int sector;
 int isEmpty;
} empleado;

int inicializarEmpleado(empleado* list, int len);

int agregarEmpleaado(empleado* list, int len, int id, char nombre[],char
apellido[],float salirio,int sector);



#endif /* ARRAYEMPLOYEES_H_ */
