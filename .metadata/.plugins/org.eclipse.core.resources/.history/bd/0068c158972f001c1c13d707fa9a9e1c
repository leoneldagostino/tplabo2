#include <stdio.h>
#include <stdlib.h>
#include "menu.h"
int opcion;

int main(void) {
	setbuf(stdout,NULL);
	do{
		menu();
		printf("Ingrese la opcion que quiere realizar:");
		scanf("%d",&opcion);
		switch(opcion){
		case 1:
			printf("ingreso a caso 1\n");
			break;
		case 2:
			printf("ingreso a caso 2\n");
			break;
		case 3:
			printf("ingreso a caso 3\n");
			break;
		case 4:
			printf("ingreso a caso 4\n");
			break;
		}
	}while(opcion!=5);

	printf("Gracias por utilizar este programa");

}
